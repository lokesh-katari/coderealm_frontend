model submissions {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  pid         String   @unique
  queId       String
  email       String
  code        String
  language    String
  testcases   Int[]
  runtime     String
  memory      String
  submittedAt DateTime @default(now())
  output      String
}

